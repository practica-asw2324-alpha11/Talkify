<div class="wotitem">
  <div class="post-header">
    <div>
      <!-- Botones de votaciÃ³n -->
      <span class="vote-buttons">
        <%= button_to 'Upvote'%>
        <%= button_to 'Downvote' %>
      </span>
    </div>

    <!-- Contenido del post -->
    <div class="post-content">
        <h2>Titulo:  <%= post.title %></h2>
