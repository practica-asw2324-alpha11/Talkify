<h2>Login</h2>


<div class="wotitem">
  <div class="post-header">
    <%= form_for(resource, as: resource_name, url: admin_session_path) do |f| %>

      <% if devise_mapping.rememberable? %>
        <div class="field">
          <%= f.check_box :remember_me %>
          <%= f.label :remember_me %>
        </div>
      <% end %>

      <div class="actions">
        <%= f.submit "Log in" %>
      </div>
    <% end %>

    <!--%= render "admins/shared/links" %>-->
        <%= link_to "Sign in with Google", admin_google_oauth2_omniauth_authorize_path %>

  </div>
</div>